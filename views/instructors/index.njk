{% extends "layout.njk" %}

{% block content%}

<div class="card table-container">
    
    <table width="100%">
        <Thead>
            <tr>
                <th>Instructors</th>
                <th>Atendimento</th>
            </tr>
        </Thead>
        <tbody>

            {% for instructor in instructors %}

            <tr>
                <td>{{instructor.name}}</td>
                <td>{{instructor.services}}</td>
            </tr>

            {% endfor %}
           
        </tbody>
    </table>

</div>

{%endblock%}